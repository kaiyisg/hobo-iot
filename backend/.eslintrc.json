{
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "extends": "airbnb-base",
  "plugins": [
      "import"
  ],
  "rules": {
      "indent": [
          "error",
          2
      ],
      "quotes": [
          "error",
          "single"
      ],
      "semi": [
          "error",
          "always"
      ],
      "max-len": [
          "error",
          120
      ],
      "no-console": 0,
      "no-param-reassign": 0
  }
}